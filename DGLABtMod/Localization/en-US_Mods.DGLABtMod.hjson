Configs: {
	MainConfig: {
		DisplayName: 客户端配置

		Headers: {
			重要安全警示: 重要安全警示
			通道设置（务必同时在APP中设置强度上限！！）: 通道设置（务必同时在 A P P中设置强度上限！！）
			强度设置: 强度设置
			服务设置: 服务设置
		}

		WARN: {
			Label: 启用此项即代表您确认您的年龄已满18周岁，且没有在此页面进行任何虚假陈述
			Tooltip: ""
		}

		WARN1: {
			Label: 启用此项即代表您确认您已阅读并遵守 声明、DG-LAB安全须知(鼠标停靠查看摘要)
			Tooltip:
				'''
				DG-LAB郊狼安全须知摘要：
				a) 已阅读并理解设备制造商的安全指南。
				b) 植入心脏起搏器者、孕妇、心脏病患者、癫痫患者、知觉异常、无法及时操作产品人群、
				    未成年人等不适宜人群严禁使用。
				c) 在安全的部位使用产品，禁止在头部、胸部、心脏附近等危险部位使用。
				d) 首次使用时从最低强度开始，在适合自身的强度范围使用。
				你也可以在DG-LAB应用设置中找到此安全须知的完整版。

				'''
		}

		WARN2: {
			Label: 启用此项即代表您确认您无心脏病、癫痫等健康禁忌，且使用前已咨询医生
			Tooltip: ""
		}

		WARN3: {
			Label: 启用此项即代表您确认您仅使用DG-LAB 3.0原厂未改装设备
			Tooltip: ""
		}

		WARN4: {
			Label: 启用此项即代表您确认您当前设置的强度在您的承受范围内，且自愿承担使用风险
			Tooltip: ""
		}

		A: {
			Label: 启用通道A
			Tooltip:
				'''
				推荐值:启用
				决定是否启用此通道
				'''
		}

		MaxA: {
			Label: 通道A强度上限
			Tooltip:
				'''
				推荐值:100
				给此通道设置强度限制,官方最大值为200
				若实时强度大于此数值,则实际强度等于此值
				例如,当最大值为100但强度已达到120时,则输出强度等于100
				'''
		}

		LowA: {
			Label: 通道A强度下限
			Tooltip:
				'''
				推荐值:0
				给此通道设置强度最低限制,官方最大值为200
				若实时强度小于此数值,则实际强度等于此值
				例如,当最小值为20但强度已达到10时,则输出强度等于20
				'''
		}

		B: {
			Label: 启用通道B
			Tooltip:
				'''
				推荐值:启用
				决定是否启用此通道
				'''
		}

		MaxB: {
			Label: 通道B强度上限
			Tooltip:
				'''
				推荐值:100
				给此通道设置强度限制,官方最大值为200
				若实时强度大于此数值,则实际强度等于此值
				例如,当最大值为100但强度已达到120时,则输出强度等于100
				'''
		}

		LowB: {
			Label: 通道B强度下限
			Tooltip:
				'''
				推荐值:0
				给此通道设置强度最低限制,官方最大值为200
				若实时强度小于此数值,则实际强度等于此值
				例如,当最小值为20但强度已达到10时,则输出强度等于20
				'''
		}

		CommStrength: {
			Label: 每十点普通伤害强度
			Tooltip:
				'''
				推荐值:3
				用于设置每受到十点伤害后增加多少强度,建议值为3
				例如,当受到42点普通伤害时,若此项设置为3,则会增加12.6点强度

				普通伤害指玩家受击后会发出叫声的伤害
				比如僵尸伤害,摔落伤害,岩浆直接伤害
				增益伤害不在普通伤害范围内
				'''
		}

		BuffStrength: {
			Label: 每秒Buff伤害强度
			Tooltip:
				'''
				推荐值:3
				用于设置获得此类Buff状态时,每秒增加多少强度

				Buff伤害指 燃烧,中毒 这类会受伤,但又不强的Buff伤害
				详见本模组README.md
				'''
		}

		BuffPlusStrength: {
			Label: 每秒强Buff伤害强度
			Tooltip:
				'''
				推荐值:6
				用于设置获得此类Buff状态时,每秒增加多少强度

				酸性毒液 和 带电(感同身受)等Buff属于 强伤害Buff
				详见本模组README.md
				'''
		}

		BuffTroubleStrength: {
			Label: 干扰型Buff增加强度
			Tooltip:
				'''
				推荐值:10
				用于设置获得此类Buff状态时,在当前强度基础上增加多少强度,不会重复叠加

				狂卷之舌 和 虚弱 等Buff属于 干扰型Buff
				详见本模组README.md
				'''
		}

		Death: {
			Label: 死亡时强度
			Tooltip:
				'''
				属于死亡界面时的固定强度,推荐值为90,
				重生后会立即恢复为0
				详见本模组README.md
				'''
		}

		DeathTime: {
			Label: 死亡时惩罚时间（1/60秒）
			Tooltip: 设置死亡后会持续输出多久.设为0则表示在死亡界面会一直输出
		}

		WaitTime: {
			Label: 强度下降等待时间
			Tooltip:
				'''
				用于设置受到伤害后等待多少秒后开始下降强度
				建议值为10
				'''
		}

		Speed: {
			Label: 强度下降速度
			Tooltip:
				'''
				用于设置每秒下降多少强度
				建议值为3
				'''
		}

		SpeedBuff: {
			Label: 普通生命回复Buff强度下降速度
			Tooltip:
				'''
				用于设置普通生命回复Buff每秒多下降多少强度
				建议值为2
				'''
		}

		SpeedBuffPlus: {
			Label: 强生命回复Buff强度下降速度
			Tooltip:
				'''
				用于设置强生命回复Buff每秒多下降多少强度
				建议值为6
				'''
		}

		Port: {
			Label: 服务启动端口
			Tooltip: 推荐值9999,如果无法生成二维码,可尝试修改此数值(需在1024-65535之间)
		}

		IP: {
			Label: 二维码连接地址
			Tooltip:
				'''
				建议留空,留空则会自动获取此电脑在局域网中的IP,
				如果无法连接,可尝试修改
				（修改需确保IP为此电脑在局域网中的IP,一般是192.168.x.xxx,可在你家里的路由器管理界面找到）
				详见本模组README.md
				'''
		}

		IsDebug: {
			Label: 启用调试
			Tooltip:
				'''
				勾选后,每当受伤或死亡事件发生时将会产生提示
				特别不建议开启
				详见本模组README.md
				'''
		}

		DebugBuff: {
			Label: 调试选项:显示Buff效果
			Tooltip:
				'''
				勾选后,Buff伤害事件发生时将会产生提示,前提是启用调试
				特别不建议开启
				详见本模组README.md
				'''
		}
	}
}
